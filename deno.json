{
  "name": "@tobert/lib",
  "version": "0.1.3",
  "license": "MIT",
  "exports": {
    "./common/*": "./src/common/*",
    "./api/*": "./src/api/*",
    "./admin/*": "./src/admin/*"
  },
  "tasks": {
    "codegen:api": "deno run -A npm:@graphql-codegen/cli@5.0.4/graphql-codegen --config codegen-api.ts",
    "codegen:admin": "deno run -A npm:@graphql-codegen/cli@5.0.4/graphql-codegen --config codegen-admin.yml"
  },
  "imports": {
    "@graphql-codegen/cli": "npm:@graphql-codegen/cli@^5.0.4"
  }
}
